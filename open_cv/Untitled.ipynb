    "import cv2\n",
    "\n",
    "path = \"lambo.jpg\"\n",
    "img = cv2.imread(path)\n",
    "\n",
    "imgHSV = cv2.cvtColor(img,cv2.COLOR_BGR2HSV)\n",
    "cv2.imshow(\"orginal\",img)\n",
    "cv2.imshow(\"hsv\",imgHSV)\n",
    "\n",
    "cv2.waitKey(0)"
